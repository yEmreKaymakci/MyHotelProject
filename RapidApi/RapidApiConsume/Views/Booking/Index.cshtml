@model List<BookingApiViewModel.Property1>

@{
    Layout = null;
	int count = 0;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<div class="container mt-5">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>#</th>
                <th>Otel Adı</th>
                <th>Otel Puanı</th>
                <th>Otel Detayları</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
				count++;
                <tr>
                    <td>@count</td>
                    <td>@item.name</td>
                    <td>@item.reviewScore</td>
                    <td><a href="#" class="btn btn-outline-dark">Detaylar</a></td>
                </tr>
            }
        </tbody>
    </table>
</div>